            </StackPanel>

            <Popup x:Name="DropdownMenu" PlacementTarget="{Binding ElementName=ButtonContainer}" IsOpen="False">
                <Border Background="White" BorderThickness="1" BorderBrush="Black">
                    <StackPanel>
                        <ListBox>
                            <ListBoxItem Content="Level 1" />
                            <ListBoxItem Content="Level 2" />
                            <ListBoxItem Content="Level 3" />
                        </ListBox>
                        <Button Content="Laden" />
                        <Button Content="Speichern" />
                        <Button Content="Abbrechen" />
                    </StackPanel>
                </Border>
            </Popup>

            <TextBlock x:Name="SelectedSpielstandTextBlock" Grid.Row="4" HorizontalAlignment="Center" Margin="0,20" />
        </Grid>
        <Grid HorizontalAlignment="Center" VerticalAlignment="Center" Visibility="Hidden">
            <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" Margin="10">
                <TextBlock Text="In Game Menü" FontSize="24" HorizontalAlignment="Center" Margin="0,0,0,20"/>

                <Button Content="Zurück zum Spiel" Click="BackToGame_Click" Margin="0,5"/>
                <Button Content="Spiel laden" Click="LoadGame_Click" Margin="0,5"/>
                <Button Content="Spiel speichern" Click="SaveGame_Click" Margin="0,5"/>
                <Button Content="Optionen" Click="Options_Click" Margin="0,5"/>
                <Button Content="Zurück zum Hauptmenü" Click="BackToMainMenu_Click" Margin="0,5"/>
                <Button Content="Programm beenden" Click="ExitProgram_Click" Margin="0,5"/>
            </StackPanel>
        </Grid>
    </Grid>
</Window>
