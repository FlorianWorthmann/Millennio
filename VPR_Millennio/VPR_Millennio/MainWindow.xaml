        <DockPanel x:Name="DockPanelBaumenu" Visibility="Hidden">
            <Grid x:Name="GridLabelBaumenu" DockPanel.Dock="Top">
                <Grid.Resources>
                    <Style TargetType="{x:Type Label}">
                        <Setter Property="FontSize" Value="10"/>
                    </Style>
                </Grid.Resources>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="9*"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <Label Grid.Row="0" Grid.Column="0" BorderThickness="3" BorderBrush="Black"  Height="75" DockPanel.Dock="Top" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="40" VerticalAlignment="Top">Gebäude</Label>
                <Button Name="btnExitBaumenu" Grid.Row="0" Grid.Column="1" FontSize="40" BorderThickness="0,3,3,3" BorderBrush="Black" Click="btnExitBaumenu_Click">X</Button>
            </Grid>
            <Border BorderThickness="3,3,3,3" BorderBrush="Black" Margin="10,10,10,10">
                <DockPanel Margin="10,10,10,10" Background="White">
                    <ScrollViewer HorizontalScrollBarVisibility="Auto">
                        <StackPanel VerticalAlignment="Top" Margin="0,-10,10,0">
                            <StackPanel.Resources>
                                <Style TargetType="{x:Type Grid}">
                                    <Setter Property="Height" Value="90"/>
                                    <Setter Property="VerticalAlignment" Value="Top"/>
                                    <Setter Property="Background" Value="Silver"/>
                                </Style>
                                <Style TargetType="{x:Type Border}">
                                    <Setter Property="BorderThickness" Value="3"/>
                                    <Setter Property="BorderBrush" Value="Black"/>
                                    <Setter Property="Margin" Value="0,10,0,0"/>
                                </Style>
                            </StackPanel.Resources>
                            <Border>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="1*" />
                                        <ColumnDefinition Width="1*" />
                                        <ColumnDefinition Width="1*" />
                                        <ColumnDefinition Width="1*" />
                                        <ColumnDefinition Width="1*" />
                                        <ColumnDefinition Width="1*" />
                                        <ColumnDefinition Width="1*" />

                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="1*" />
                                        <RowDefinition Height="1*" />
                                        <RowDefinition Height="0.1*" />
                                        <RowDefinition Height="1*" />
                                        <RowDefinition Height="1*" />

                                    </Grid.RowDefinitions>
                                    <Grid>

                                    </Grid>
                                    <Label Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="3">Name</Label>
                                    <Label Grid.Column="1" Grid.Row="0">Name</Label>
                                </Grid>
                            </Border>
                            <Border>
                                <Grid>
                                </Grid>
                            </Border>
                            <Border>
                                <Grid>
                                </Grid>
                            </Border>
                            <Border>
                                <Grid>
                                </Grid>
                            </Border>
                            <Border>
